int main() {
    let a := 5;
    var b := 4;
    b += add(add_five(random()),  2);
    return b;
}

// A strange function that adds five to its argument and returns it. Why not directly 
// return a + 5 ? Because i need to test mutable parameters :)
int add_five(mut a: int) {
    a += 5;     
    return a;
}

int add(a: int, b: int) {
    return a + b;
}

// See https://xkcd.com/221/
int random() {
    return 4;   // Chosen by fair dice roll, guaranteed to be random.
}
