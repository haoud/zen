int main() {
    let a := 5;
    var b := 4;
    b += add(add_five(random()),  2);
    return b;
}

int add_five(a: int) {
    return a + 5;
}

int add(a: int, b: int) {
    return a + b;
}

// See https://xkcd.com/221/
int random() {
    return 4;   // Chosen by fair dice roll, guaranteed to be random.
}
