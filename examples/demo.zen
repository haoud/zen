struct data {
    metadata: struct metadata,
    value: bool,
}

struct metadata {
    alignment: int,
    size: int,
}

int init_data(mut data: struct data) {
    data.metadata.alignment = 8;
    data.metadata.size += 16;
    data.value = false;

    return data.metadata.size;
}

